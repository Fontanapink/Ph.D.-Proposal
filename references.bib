@article{huang_EffectsPromoterLeakage_2015,
  title = {Effects of Promoter Leakage on Dynamics of Gene Expression},
  author = {Huang, Lifang and Yuan, Zhanjiang and Liu, Peijiang and Zhou, Tianshou},
  date = {2015-03-21},
